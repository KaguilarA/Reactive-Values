{
  "name": "reactive-values",
  "version": "1.0.14",
  "description": "Lightweight reactive value library with support for deep equality and computed values.",
  "type": "module",
  "author": "Kevin Aguilar",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite --config vite.demo.config.ts",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "build-demo": "tsc -p tsconfig.demo.json && vite build --config vite.demo.config.ts",
    "demo": "vite --config vite.demo.config.ts",
    "preview-demo": "vite preview --config vite.demo.config.ts"
  },
  "keywords": [
    "reactive",
    "state-management",
    "typescript",
    "computed",
    "observer",
    "state",
    "reactivity",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KaguilarA/Reactive-Values.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "homepage": "https://kaguilara.github.io/Reactive-Values/",
  "devDependencies": {
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "jsdom": "26.1.0",
    "typescript": "~5.8.3",
    "vite": "7.1.2",
    "vite-plugin-dts": "4.5.4",
    "vitest": "^3.2.4"
  },
  "license": "MIT"
}
